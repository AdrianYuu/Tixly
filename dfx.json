{
  "canisters": {
    "backend_user": {
      "type": "motoko",
      "main": "backend/user/main.mo"
    },
    "backend_activity": {
      "type": "motoko",
      "main": "backend/activity/main.mo"
    },
    "backend_concert": {
      "type": "motoko",
      "main": "backend/concert/main.mo"
