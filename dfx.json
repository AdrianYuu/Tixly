    "backend_movie": {
      "type": "motoko",
      "main": "backend/movie/main.mo"
    },
    "backend_tourist_attraction": {
      "type": "motoko",
      "main": "backend/tourist_attraction/main.mo"
    },
    "backend_concert_ticket_type": {
